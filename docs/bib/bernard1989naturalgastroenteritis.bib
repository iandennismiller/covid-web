    volume = {21},
    number = {1},
    pages = {1-8},
    doi = {10.1016/0378-1135(89)90013-8}
    citedbycount = {22},
    abstract = {Our objective was to evaluate the level of passive protection against transmissible gastroenteritis (TGE) among 57 newborn piglets nursing from seven seropositive sows previously naturally infected with porcine respiratory coronavirus (PRCV). After challenge exposure we observed mortality rates of 44% for litters of seven PRCV-infected sows, 40% for litters of four sows orally immunized with the attenuated TGEV strain Nouzilly, and 91% for litters of seven seronegative susceptible sows. A blocking ELISA with two appropriate monoclonal antibodies distinguished serological responses of PRCV-infected sows from those of TGEV-immunized sows. The results suggest that natural infection of the sow with PRCV may induce a degree of protective lactogenic immunity against TGE. Â© 1989.},
    keywords = {newborn piglets, monoclonal antibodies, results suggest, porcine respiratory, respiratory coronavirus, transmissible gastroenteritis}
}

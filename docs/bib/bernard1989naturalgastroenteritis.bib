    author = {Bernard, S. and Bottreau, E. and Aynaud, J. M. and Have, P. and Szymansky, J.},
    title = {Natural infection with the porcine respiratory coronavirus induces protective lactogenic immunity against transmissible gastroenteritis},
    journal = {Veterinary Microbiology},

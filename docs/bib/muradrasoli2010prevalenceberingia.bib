    author = {Muradrasoli, Shaman and Bálint, Ádám and Wahlgren, John and Waldenström, Jonas and Belák, Sándor and Blomberg, Jonas and Olsen, Björn},
    title = {Prevalence and phylogeny of coronaviruses in wild birds from the bering strait area (Beringia)},
    journal = {PLoS ONE},

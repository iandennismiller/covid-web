@article{Möstl1988IncidenceCoronavirus,
    author = {Möstl, K. and Bürki, F.},
    title = {Incidence of diarrhoea and of rotavirus‐ and coronavirus‐shedding in calves, whose dams had been vaccinated with an experimental oil‐adjuvanted vaccine containing bovine rotavirus and bovine coronavirus},
    journal = {Journal of Veterinary Medicine, Series B},

    author = {Lin, Cheng Wen and Tsai, Fuu Jen and Wan, Lei and Lai, Chien Chen and Lin, Kuan Hsun and Hsieh, Tsung Han and Shiu, Shi Yi and Li, Jeng Yi},
    title = {Binding interaction of SARS coronavirus 3CLpro protease with vacuolar-H+ ATPase G1 subunit},

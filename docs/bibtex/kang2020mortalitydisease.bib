@article{Kang2020MortalityDisease,
    author = {Kang, Yun-Jung},
    title = {Mortality rate of infection with COVID-19 in Korea from the perspective of underlying disease.},
    journal = {Disaster Med Public Health Prep},
    issn = {},
    year = {2020},
    volume = {},
    number = {},
    pages = {},
    doi = {10.1017/dmp.2020.60}
    citedbycount = {0},
    abstract = {On December 31st, 2019 the China National Health Commission (NHC) reported that an unknown cause of pneumonia had been detected in Wuhan in Hubei province. On February 12th, the disease caused by novel coronavirus (2019-nCoV) has a formal name, COVID-19. On January 20th, 2020, the first case of COVID-19 was confirmed in Korea. Among the deaths, age-specific death rate was the highest among cases over 70's, with underlying diseases in their circulatory system, such as myocardial infraction, cerebral infraction, arrythmia, and hypertension. Patients with underlying disease who are 70 years of age or older should recognize that there is a high possibility of developing a serious disease in case of viral infection and follow strict precautions.},
    keywords = {COVID-19, Mortality rate, Over 70, Perspective, Prevention, Underlying diseases, novel coronavirus}
}

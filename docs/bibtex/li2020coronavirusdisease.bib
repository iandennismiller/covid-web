    author = {Li, Mingzhi and Lei, Pinggui and Zeng, Bingliang and Li, Zongliang and Yu, Peng and Fan, Bing and Wang, Chuanhong and Li, Zicong and Zhou, Jian and Hu, Shaobo and Liu, Hao},
    title = {Coronavirus Disease (COVID-19): Spectrum of CT Findings and Temporal Progression of the Disease.},
